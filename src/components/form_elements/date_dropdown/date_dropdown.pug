mixin date_dropdown(enter_label, outgo_label)
  .date_dropdown_label_input
    .enter_date_dropdown 
      label.date_form_label(for="enter")= enter_label
      input.datepicker-here.date_dropdown_input(type="text", name="")
    .output_date_dropdown
      label.date_form_label(for="outgo")= outgo_label
      input.date_dropdown_input(type="text", name="")